{
  "name": "icloud-photos-sync",
  "version": "0.2.0",
  "description": "One-way sync engine for iCloud Photos Library within the native file system",
  "preferGlobal": true,
  "type": "module",
  "main": "bin/main.js",
  "types": "bin/main.d.ts",
  "bin": {
    "icloud-photos-sync": "bin/main.js"
  },
  "scripts": {
    "prebuild-clean": "echo \"// File created during pre-build, to expose package metadata to the application\" > src/lib/package.ts",
    "prebuild-name": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" >> src/lib/package.ts",
    "prebuild-version": "node -p \"'export const NAME = ' + JSON.stringify(require('./package.json').name) + ';'\" >> src/lib/package.ts",
    "prebuild-description": "node -p \"'export const DESC = ' + JSON.stringify(require('./package.json').description) + ';'\" >> src/lib/package.ts",
    "prebuild": "npm run prebuild-clean && npm run prebuild-name && npm run prebuild-version && npm run prebuild-description",
    "build": "npm install && eslint --fix --ext .ts ./src && tsc",
    "docs": "npx typedoc",
    "clean": "rm -rf bin/ docs/ node_modules/",
    "test": "echo \"Currently no test specified\" && exit 0",
    "execute": "node bin/main.js",
    "start": "npm run build && npm run execute"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steilerDev/icloud-photos-sync.git"
  },
  "keywords": [
    "icloud",
    "icloud-sync",
    "icloud-photos"
  ],
  "author": "Frank Steiler",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/steilerDev/icloud-photos-sync/issues"
  },
  "homepage": "https://github.com/steilerDev/icloud-photos-sync#readme",
  "engines": {
    "node": ">=18.6.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^17.0.42",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "typedoc": "^0.23.5",
    "typedoc-github-wiki-theme": "^1.0.1",
    "typedoc-plugin-markdown": "^3.13.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.0",
    "cli-progress": "^3.11.2",
    "commander": "^9.0.0",
    "loglevel": "^1.8.0",
    "p-event": "^5.0.1",
    "p-queue": "^7.2.0",
    "tough-cookie": "^4.0.0"
  }
}

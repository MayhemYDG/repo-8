{
  "name": "icloud-photos-sync",
  "version": "0.0.4",
  "description": "Sync engine for iCloud Photos library within the native file system",
  "preferGlobal": true,
  "type": "module",
  "main": "bin/main.js",
  "types": "bin/main.d.ts",
  "bin": {
    "icloud-photos-sync": "bin/main.js"
  },
  "scripts": {
    "build": "eslint --fix --ext .ts ./src && tsc",
    "build-watch": "npm run build -- -w",
    "docs": "npx typedoc",
    "clean": "rm -rf bin/ docs/",
    "test": "echo \"Currently no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steilerDev/icloud-photos-sync.git"
  },
  "keywords": [
    "icloud",
    "icloud-photos"
  ],
  "author": "Frank Steiler",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/steilerDev/icloud-photos-sync/issues"
  },
  "homepage": "https://github.com/steilerDev/icloud-photos-sync#readme",
  "engines": {
    "node": ">=17.4.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "typedoc": "^0.22.11",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.13",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "commander": "^9.0.0",
    "loglevel": "^1.8.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "needle": "^3.0.0"
  }
}
